<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true">
        <color android:color="#FF0000"/>

    </item>

    <item >
        <shape>
            <solid
                android:color="#FB7C16" />
            <stroke
                android:width="3dp"
                android:color="#0000FF" />
            <corners
                android:radius="3dp" />
        </shape>

    </item>
</selector>